package modelo;

import java.util.ArrayList;
import java.util.List;

public class ListaCompra {

	private List<ItemListaCompra> lista;

	public ListaCompra() {
		super();
		this.lista = new ArrayList<ItemListaCompra>();
	}

	public List<ItemListaCompra> getLista() {
		return lista;
	}
	
	public void addItemListaCompra(ItemListaCompra i) {
		lista.add(i);
	}
	
}
