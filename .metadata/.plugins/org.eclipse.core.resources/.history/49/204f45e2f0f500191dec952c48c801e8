package modelo;

public class ProductoPrecio {

	private int id;
	private float precio;
	
	public ProductoPrecio(int id, float precio) {
		super();
		this.id = id;
		this.precio = precio;
	}

	public int getId() {
		return id;
	}

	public float getPrecio() {
		return precio;
	}
	
	
}
