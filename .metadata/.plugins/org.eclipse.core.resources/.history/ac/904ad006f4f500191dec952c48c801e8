package modelo;

import java.util.ArrayList;
import java.util.List;

public class Lista {

	private int id;
	private String nombre;
	private String descripcion;
	private List<ItemLista> lista;

	public Lista() {
		super();
		this.lista = new ArrayList<ItemLista>();
	}

	public List<ItemLista> getLista() {
		return lista;
	}
	
	public void addItemListaCompra(ItemLista i) {
		lista.add(i);
	}
	
}
